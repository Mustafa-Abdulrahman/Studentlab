
@model IEnumerable<AddStudentVM>




	<table >
		<tr>
			<th>Id</th>
			<th>First Name</th>
			<th>Last Name</th>

			<th>Email</th>
			<th>Age</th>
			<th>Department</th>
			<th>Iq Level</th>
			<th>Update</th>
			<th>Delete</th>
		</tr>
	@foreach (var item in Model)
	{
				<tr>
						<td>@item.Id</td>
						<td>@item.fName</td>
						<td>@item.lName</td>
						<td>@item.Email</td>
						<td>@item.Age</td>
							<td>@item.Department</td>
							<td>@item.IQLevel</td>
						<td ><a  target="_self" href="Edit/@item.Id">Update</a></td>
						<td ><a  target="_self" href="MainPage/del/@item.Id">Delete</a></td>
				</tr>
	}

	</table>